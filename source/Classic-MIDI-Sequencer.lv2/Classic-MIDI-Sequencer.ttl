@prefix atom: <http://lv2plug.in/ns/ext/atom#> .
@prefix doap: <http://usefulinc.com/ns/doap#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix lv2:  <http://lv2plug.in/ns/lv2core#> .
@prefix midi: <http://lv2plug.in/ns/ext/midi#> .
@prefix mod:  <http://moddevices.com/ns/mod#> .
@prefix rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix time: <http://lv2plug.in/ns/ext/time#> .
@prefix epp:   <http://lv2plug.in/ns/ext/port-props/#>.

<http://moddevices.com/plugins/mod-devel/Classic-MIDI-Sequencer>
        a mod:MIDIPlugin ,
            lv2:UtilityPlugin ,
            lv2:Plugin ;
        doap:name "Classic MIDI Sequencer" ;
        doap:license "GPLv2+" ;
        rdfs:comment """
MIDI version of the MOD SwitchBox.
This switch box receives a MIDI input and channels it through one of its two outputs.""" ;
        lv2:minorVersion 2 ;
        lv2:microVersion 0 ;
        lv2:optionalFeature lv2:hardRTCapable ;
        lv2:port [
                a lv2:InputPort ,
                        atom:AtomPort ;
                atom:bufferType atom:Sequence ;
                atom:supports midi:MidiEvent ;
                lv2:index 0 ;
                lv2:symbol "in" ;
                lv2:name "In" ;
        ] , [
                a lv2:OutputPort ,
                        atom:AtomPort ;
                atom:bufferType atom:Sequence ;
                atom:supports midi:MidiEvent ;
                lv2:index 1 ;
                lv2:symbol "out1" ;
                lv2:name "Out 1" ;
        ] , [
					      a lv2:InputPort ,	atom:AtomPort ;
								atom:bufferType atom:Sequence ;
								atom:supports time:Position ;
								lv2:index 2 ;
								lv2:symbol "control" ;
								lv2:name "Control" ;
       ] , [     a lv2:InputPort, lv2:ControlPort ;
       					 lv2:index 3 ;
       					 lv2:name "mode" ;
								 lv2:symbol "mode" ;
								 lv2:default 2.000000 ;
								 lv2:minimum 0.500000 ;
								 lv2:maximum 4.000000 ;
								 lv2:scalePoint [ rdfs:label "Clear All" ; rdf:value 0.000000 ] ;
								 lv2:scalePoint [ rdfs:label "Record" ; rdf:value 1.000000  ] ;
								 lv2:scalePoint [ rdfs:label "Play" ; rdf:value 2.000000 ] ;
                 lv2:scalePoint [ rdfs:label "Record Append" ; rdf:value 3.000000 ] ;
                 lv2:scalePoint [ rdfs:label "Undo Last" ; rdf:value 4.000000 ] ;
                 ] ;

        doap:developer [
            foaf:name "Bram Giesen" ;
            foaf:homepage <http://bramgiesen.com> ;
            foaf:mbox <bram@moddevices.com> ;
        ] ;

        doap:maintainer [
            foaf:name "MOD Team" ;
            foaf:homepage <http://moddevices.com> ;
            foaf:mbox <mailto:devel@moddevices.com> ;
        ] ;

        mod:brand "MOD" ;
        mod:label "Classic MIDI Sequencer" .
